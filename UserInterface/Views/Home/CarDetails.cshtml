            }
            @if (@Model.AutoMarketValue != null)
            {
                if (!string.IsNullOrEmpty(Model.AutoMarketValue.vin))
                {
                    if (Model.ExtProperties != null)
                    {
                        if (Model.ExtProperties.ShowMarketVal != null)
                        {
                            if (Model.ExtProperties.ShowMarketVal == true)
                            {
