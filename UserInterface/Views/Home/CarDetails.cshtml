            <div id="menu3" class="tab-pane fade">
                <p style="word-break: break-all">
                    <h3 style="padding-left:20px; color:black;">Specifications</h3>
                    <br />
                    <ul class="list_details">
                        @{
                            var des = Model.Description;

                            if (!string.IsNullOrEmpty(des))
                            {
                                if (des.Contains(';') & des.Contains(':'))
                                {
                                    string[] arrDesc = des.Split(';');
                                    foreach (string item in arrDesc)
                                    {

                                        string[] arrSentenece = item.Split(':');

                                        var part1 = arrSentenece[0];
                                        var part2 = arrSentenece[1];

                                        if (part1.ToLower() != "details")
                                        {
                                            var temp1 = part1[0].ToString().ToUpper() + part1.Substring(1);
                                            var temp2 = part2[0].ToString().ToUpper() + part2.Substring(1);

                                            var temp = temp1 + " : " + temp2;

                                            <li>@temp</li>
                                        }


                                    }
                                }
                                else
                                {
                                    var temp = des;
                                    <li>@temp</li>
                                }

                            }
                            else
                            {
                                var temp = "Classified car, private party no description available.";
                                <li>@temp</li>
                            }


                        }
                        @*@Html.Raw(HttpUtility.UrlDecode(Model.Description))*@
